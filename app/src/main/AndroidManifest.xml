<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.josheasop.characterkeeper">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomePage"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewDD5E"
            android:label="@string/title_activity_new_dd5_e"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TestDisplay"
            android:label="@string/title_activity_test_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DD5E_Edit"
            android:label="@string/title_activity_dd5_e__edit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dd5e_equipment_edit"
            android:label="@string/title_activity_dd5e_equipment_edit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dd5e_equipment"
            android:label="@string/title_activity_dd5e_equipment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dd5e_display"
            android:label="@string/title_activity_dd5e_display"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>